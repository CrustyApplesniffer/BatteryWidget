    <!-- App Info Text Sections -->
    <string name="app_title">Battery Widget - Complete Guide</string>
    <string name="app_welcome">Welcome to Battery Widget! This app provides a beautiful, functional battery monitoring widget for your home screen.</string>

    <string name="features_title">üìä Features Overview:</string>
    <string name="real_time_monitoring">Real-time Monitoring</string>
    <string name="monitoring_bullet_points">‚Ä¢ Always see your current battery level\n‚Ä¢ Live updates with accurate percentage\n‚Ä¢ No delays or lag in battery information</string>

    <string name="visual_design">üé® Visual Design</string>
    <string name="design_bullet_points">‚Ä¢ Dynamic color coding:\n  &#8211; Green (80&#8211;100%%): High battery\n  &#8211; Light Green (60&#8211;79%%): Good battery\n  &#8211; Yellow (40&#8211;59%%): Medium battery\n  &#8211; Orange (20&#8211;39%%): Low battery\n  &#8211; Red (0&#8211;19%%): Critical battery\n‚Ä¢ Clean, minimalist design\n‚Ä¢ Smooth progress bar animation</string>

    <string name="smart_functionality">‚ö° Smart Functionality</string>
    <string name="functionality_bullet_points">‚Ä¢ Charging status indicator (flash icon)\n‚Ä¢ Auto-updates when battery changes\n‚Ä¢ Manual refresh intervals available\n‚Ä¢ Tap widget to open battery settings</string>

    <string name="configuration_options">‚öôÔ∏è Configuration Options</string>
    <string name="configuration_bullet_points">‚Ä¢ Auto Mode: Updates on battery changes\n‚Ä¢ 30 Seconds: Frequent updates\n‚Ä¢ 1 Minute: Balanced updates\n‚Ä¢ 15 Minutes: Less frequent updates\n‚Ä¢ 30 Minutes: Power-saving updates</string>

    <string name="installation_usage">üè† Installation &amp; Usage</string>
    <string name="adding_widget">Adding the Widget:</string>
    <string name="widget_steps">1. Long-press on your home screen\n2. Select \"Widgets\" from the menu\n3. Scroll to find \"Battery Widget\"\n4. Drag and drop to your desired location\n5. The widget will automatically start working</string>

    <string name="configuring_widget">Configuring the Widget:</string>
    <string name="configuration_tips">‚Ä¢ Tap the widget to open configuration\n‚Ä¢ Choose your preferred update interval\n‚Ä¢ Settings are saved automatically</string>

    <string name="battery_settings_access">üîã Battery Settings Access</string>
    <string name="settings_options">One tap on the widget opens your device\'s battery settings, where you can:\n‚Ä¢ View detailed battery usage\n‚Ä¢ Enable battery saver mode\n‚Ä¢ Check which apps use most power\n‚Ä¢ Optimize battery settings</string>

    <string name="privacy_permissions">üîí Privacy &amp; Permissions</string>
    <string name="privacy_points">‚Ä¢ No internet connection required\n‚Ä¢ No personal data collection\n‚Ä¢ Only reads battery level and charging status\n‚Ä¢ All data stays on your device\n‚Ä¢ No ads or tracking</string>

